    "import numpy as np\n",
    "\n",
    "arr = np.array([1.1, 3.3, 5.5, 7.7, 0.0])\n",
    "\n",
    "intarr = arr.astype('i')\n",
    "boolarr = arr.astype(bool)\n",
    "\n",
    "arrays = [arr, intarr, boolarr]\n",
    "\n",
    "for array in arrays:\n",
    "    print(array)\n",
    "    print(array.dtype)"
